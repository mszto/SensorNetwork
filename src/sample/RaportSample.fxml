<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RaportControler">
   <children>
      <TableView fx:id="raportList" prefHeight="600.0" prefWidth="600.0">
        <columns>
          <TableColumn id="1" prefWidth="96.0" text="Id Sensor" />
          <TableColumn id="2" prefWidth="203.0" text="ilość nasłuchiwannych czujnikó" />
            <TableColumn id="3" prefWidth="171.0" text="odczytanych danych" />
            <TableColumn id="4" prefWidth="164.0" text="czas działania(m:s:ms)" />
            <TableColumn id="5" prefWidth="75.0" text="poziom baterii" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
